                    android:layout_height="match_parent"
                    android:fontFamily="@font/arimo_regular"
                    android:gravity="center"
                    tools:ignore="TooManyViews" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="end"
                    tools:ignore="InefficientWeight">

                    <TextView
                        android:id="@+id/date_start"
                        style="@style/style_sec_sec"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:background="@android:color/transparent"
                        android:drawableEnd="@drawable/date_icon"
                        android:drawablePadding="@dimen/tv_drawable_padding"
                        android:gravity="center"
                        android:textSize="@dimen/medium_text_low"
                        tools:ignore="SpUsage" />

                    <TextView
                        android:id="@+id/time_start"
                        style="@style/style_sec_sec"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:drawableEnd="@drawable/watch_icon"
                        android:drawablePadding="@dimen/tv_drawable_padding"
                        android:gravity="center"
                        android:textSize="@dimen/medium_text_low"
                        android:visibility="gone"
                        tools:ignore="SpUsage" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/input_field_height"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/schel_end"
                    style="@style/style_sec_sec"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/arimo_regular"
                    android:gravity="center" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="end"
                    tools:ignore="InefficientWeight">

                    <TextView
                        android:id="@+id/date_end"
                        style="@style/style_sec_sec"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:background="@android:color/transparent"
                        android:drawableEnd="@drawable/date_icon"
                        android:drawablePadding="@dimen/tv_drawable_padding"
                        android:gravity="center"
                        android:textSize="@dimen/medium_text_low"
                        tools:ignore="SpUsage" />

                    <TextView
                        android:id="@+id/time_end"
                        style="@style/style_sec_sec"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:drawableEnd="@drawable/watch_icon"
                        android:drawablePadding="@dimen/tv_drawable_padding"
                        android:gravity="center"
                        android:textSize="@dimen/medium_text_low"
                        android:visibility="gone"
                        tools:ignore="SpUsage" />
                </LinearLayout>
            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/quote_notes_layout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/input_field_height"
                android:gravity="center"
                android:paddingTop="5dp"
                app:hintAnimationEnabled="true"
                app:hintEnabled="false"
                app:hintTextAppearance="@style/TextInputLayoutHintText">

                <EditText
                    android:id="@+id/quote_notes_edt"
                    style="@style/style_sec_sec"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:color/transparent"
                    android:singleLine="true"
                    tools:ignore="Autofill,LabelFor" />
            </com.google.android.material.textfield.TextInputLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/quote_term_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="5dp"
                app:hintAnimationEnabled="true"
                app:hintEnabled="false"
                app:hintTextAppearance="@style/TextInputLayoutHintText">

                <EditText
                    android:id="@+id/quote_term_edt"
                    style="@style/style_sec_sec"
                    android:paddingTop="3dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@android:color/transparent"
                    android:gravity="top|start"
                    android:inputType="textMultiLine"
                    tools:ignore="Autofill,LabelFor" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>
    </ScrollView>


    <Button
        android:id="@+id/submit_btn"
        style="@style/style_sec"
        android:layout_width="match_parent"
        android:layout_height="@dimen/button_field_height"
        android:layout_alignParentBottom="true"
        android:layout_margin="5dp"
        android:background="@drawable/submit_btn"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textStyle="bold">

    </Button>

</RelativeLayout>
</layout>