                android:layout_height="40dp"
                android:orientation="horizontal"
                android:weightSum="2">


                <TextView
                    android:id="@+id/equ_bar_code_num_lable"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/black"
                    tools:ignore="RtlSymmetry" />

                <TextView
                    android:id="@+id/equ_bar_code_num_txt"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
                    android:layout_weight="1"
                    android:gravity="end|center_vertical"
                    android:paddingTop="5dp"
                    android:paddingEnd="10dp"
                    android:textStyle="normal"
                    tools:ignore="RtlSymmetry" />


            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:weightSum="2">


                <TextView
                    android:id="@+id/equipment_group"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/black"
                    tools:ignore="RtlSymmetry" />

                <TextView
                    android:id="@+id/equipment_group_detail"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
                    android:layout_weight="1"
                    android:gravity="end|center_vertical"
                    android:paddingTop="5dp"
                    android:paddingEnd="10dp"
                    android:textStyle="normal"
                    tools:ignore="RtlSymmetry" />


            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:weightSum="2">


                <TextView
                    android:id="@+id/custom_filed_1"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/black"
                    tools:ignore="RtlSymmetry" />

                <TextView
                    android:id="@+id/custom_filed_txt_1"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
                    android:layout_weight="1"
                    android:gravity="end|center_vertical"
                    android:paddingTop="5dp"
                    android:paddingEnd="10dp"
                    android:textStyle="normal"
                    tools:ignore="RtlSymmetry" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:weightSum="2">


                <TextView
                    android:id="@+id/custom_filed_2"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/black"
                    tools:ignore="RtlSymmetry" />

                <TextView
                    android:id="@+id/custom_filed_txt_2"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
                    android:layout_weight="1"
                    android:gravity="end|center_vertical"
                    android:paddingTop="5dp"
                    android:paddingEnd="10dp"
                    android:textStyle="normal"
                    tools:ignore="RtlSymmetry" />


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:weightSum="2">


                <TextView
                    android:id="@+id/supplier_txt"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/black"
                    tools:ignore="RtlSymmetry" />

                <TextView
                    android:id="@+id/supplier"
                    style="@style/style_sec"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="2dp"
                    android:layout_weight="1"
                    android:gravity="end|center_vertical"
                    android:paddingTop="5dp"
                    android:paddingEnd="10dp"
                    android:textStyle="normal"
                    tools:ignore="RtlSymmetry" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="@color/border_font_color" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/tv_upload_barcode"
                style="@style/style_sec"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/white_corner_radius_btn"
                android:drawableStart="@drawable/ic_qr_code"
                android:drawablePadding="5dp"
                android:drawableTint="@color/black"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:textAllCaps="false"
                android:textColor="@color/black"
                tools:ignore="UnusedAttribute" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_barcode_note"
                style="@style/style_thrid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginBottom="10dp"
                android:padding="5dp" />

            <!--part for Equipment-->
            <androidx.cardview.widget.CardView
                android:visibility="gone"
                android:id="@+id/part_cardview"
                android:layout_width="match_parent"
                app:cardBackgroundColor="@color/white"
                android:layout_margin="10dp"
                android:layout_height="wrap_content"
                >
                <LinearLayout
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_label_part"
                        style="@style/style_sec"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/attachment"
                        android:textStyle="bold" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerView_part"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:nestedScrollingEnabled="false"
                        />

                </LinearLayout>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:visibility="gone"
                android:id="@+id/item_cardview"
                android:layout_width="match_parent"
                app:cardBackgroundColor="@color/white"
                android:layout_margin="10dp"
                android:layout_height="wrap_content"
                >
