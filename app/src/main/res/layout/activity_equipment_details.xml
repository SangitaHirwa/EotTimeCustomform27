                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingStart="10dp"
                            android:paddingTop="5dp"
                            android:text="@string/equipment_audit"
                            android:textColor="@color/black"
                            tools:ignore="RtlSymmetry" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/audit_list"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            tools:listitem="@layout/adpter_aduit_history" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/job_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/job_history_txt"
                            style="@style/header_text_style"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingStart="10dp"
                            android:paddingTop="5dp"
                            android:text="@string/equipment_s_service"
                            android:textColor="@color/black"

                            tools:ignore="RtlSymmetry" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/job_list"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            tools:listitem="@layout/adpter_aduit_history" />

                    </LinearLayout>
                </LinearLayout>

            </FrameLayout>


        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

    <LinearLayout
        android:id="@+id/ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:padding="5dp">

        <Button
            android:id="@+id/button_job"
            style="@style/style_sec"
            android:layout_width="0dp"
            android:layout_height="@dimen/button_field_height"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/submit_btn"
            android:singleLine="true"
            android:text="@string/go_to_job"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button
            android:id="@+id/button_audit"
            style="@style/style_sec"
            android:layout_width="0dp"
            android:layout_height="@dimen/button_field_height"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/submit_btn"
            android:singleLine="true"
            android:text="@string/go_to_audit"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <Button
            android:id="@+id/go_to_addjob"
            style="@style/style_sec"
            android:layout_width="0dp"
            android:layout_height="@dimen/button_field_height"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/submit_btn"
            android:singleLine="true"
            android:text="@string/add_job"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:visibility="gone" />
    </LinearLayout>


</RelativeLayout>
</layout>